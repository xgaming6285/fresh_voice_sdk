# MongoDB Configuration
# For MongoDB Atlas (cloud), use the full connection string:
MONGO_DB=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=Cluster0

# OR for local MongoDB, use host and port:
# MONGODB_HOST=localhost
# MONGODB_PORT=27017

# Database name
MONGODB_DATABASE=voice_agent_crm

# Google API Keys (for Gemini API)
# Primary key (required)
GOOGLE_API_KEY=your_api_key_here

# Additional keys for concurrent call support (optional)
# Assign one key per user (admin/agent) to prevent RTP interference
GOOGLE_API_KEY_2=your_second_api_key_here
GOOGLE_API_KEY_3=your_third_api_key_here
GOOGLE_API_KEY_4=your_fourth_api_key_here
GOOGLE_API_KEY_5=your_fifth_api_key_here
GOOGLE_API_KEY_6=your_sixth_api_key_here
GOOGLE_API_KEY_7=your_seventh_api_key_here
GOOGLE_API_KEY_8=your_eighth_api_key_here
GOOGLE_API_KEY_9=your_ninth_api_key_here
GOOGLE_API_KEY_10=your_tenth_api_key_here

